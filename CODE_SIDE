 "att_storage_per_price": "STORAGE PER DOLLAR",
 "att_screen_size": "SCREEN SIZE",
 "att_screen_size_per_price": "INCHES PER DOLLAR",
 "att_weight": "WEIGHT",
 "att_ppi": "PPI"
};

